# This is an example file. Create a .env file in the same directory with actual values.
# For Pike (merchant) and Barracuda (admin) interfaces

# API Configuration
REACT_APP_API_URL=https://api.example.com/v1

# Auth0 Authentication Configuration - v2.0.0
REACT_APP_AUTH0_DOMAIN=your-tenant.auth0.com
REACT_APP_AUTH0_CLIENT_ID=your-client-id
REACT_APP_AUTH0_AUDIENCE=https://api.example.com

# Environment Information
REACT_APP_ENVIRONMENT=development # Options: development, staging, production
REACT_APP_VERSION=1.0.0

# Error Tracking
REACT_APP_SENTRY_DSN=https://examplekey@sentry.io/123456

# Monitoring Configuration
REACT_APP_DATADOG_APPLICATION_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
REACT_APP_DATADOG_CLIENT_TOKEN=pubxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Feature Flags
REACT_APP_FEATURE_REPORTING=true
REACT_APP_FEATURE_ANALYTICS=true

# Note: Additional environment-specific variables may be required.
# Refer to the documentation for complete configuration details.